{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "esModuleInterop": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "outDir": "./dist",
    "rootDir": "../",
    "baseUrl": ".",
    "paths": {
      "@muzer/auth-utils": ["muzer/lib/auth-utils"]
    },
    "moduleResolution": "node",
    "resolveJsonModule": true
  },
  "include": ["src","muzer/lib", "dist/utils.js"]
}
